{
	"python.testing.pytestArgs": ["__tests__"],
	"python.testing.unittestEnabled": false,
	"python.testing.pytestEnabled": true,
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/__pycache__": true,
		"**/*.pyc": true,
		"**/*.pyo": true,
		"**/*.pyd": true,
		"**/.pytest_cache": true,
		"**/build": true,
